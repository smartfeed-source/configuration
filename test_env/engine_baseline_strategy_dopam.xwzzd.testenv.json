     "personalization": [/*个性化推荐配置*/
         {
            "channel": "rcmd",/*频道*/
            "plans":[
                {
                    "name": "article_personalization",
                    "keyword_max_count_per_slide": 2,/*每刷最多出多少条包含同一个关键词的内容*/
                    "category_max_count_per_slide": 2,/*同一个分类每刷最多出多少条*/
                    "author_max_count_per_slide": 1,/*每个作者每刷最多出多少条*/
                    "explore_expliot_weight": 0.5,/*让引擎更多地探索新兴趣(可能导致推送的内容不是用户喜欢的)还是曝光兴趣内容(可能导致用户很快厌倦),1是全explore, 0是全expliot*/ 
                    "restrict_category":false,/*为true则个性化限制在default_strategy所设置的category范围内,false为不限制*/
                    "seats":0,/*所占槽位,个性化优先级高于default_setting,所以直接抢占它的槽位,在个性化内容质量较低时,才由他补充*/
                    "engine":{/*个性化引擎设置*/
                        "ranking": {
                            "algo": "w2v",
                            "hot_preferred_categorys": ["国际", "军事", "社会", "时政", "科技", "财经", "娱乐", "要闻", "新闻", "热点"]/*排序时需要考虑时间热度的分类*/
                        },/*精排引擎设置*/
                        "proposing": {"algo": null},/*粗排设置设置*/
                        "recall": {"category":{},"oceanus":{}}/*召回路由配置*/
                    },
                    "source":["yidian_news"],/*个性化数据源范围,当前只支持baidu_news和baidu_video*/
                    "branches":[/*配置分支,可以根据condition设置的条件命中用户，对命中的用户以配置覆盖个性化配置*/
